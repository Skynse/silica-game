[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1

[libraries]
linux.debug.x86_64 = "res://rust/silica-engine/target/debug/libsilica_godot.so"
linux.release.x86_64 = "res://rust/silica-engine/target/release/libsilica_godot.so"
windows.debug.x86_64 = "res://rust/silica-engine/target/debug/silica_godot.dll"
windows.release.x86_64 = "res://rust/silica-engine/target/release/silica_godot.dll"
macos.debug = "res://rust/silica-engine/target/debug/libsilica_godot.dylib"
macos.release = "res://rust/silica-engine/target/release/libsilica_godot.dylib"
macos.debug.arm64 = "res://rust/silica-engine/target/debug/libsilica_godot.dylib"
macos.release.arm64 = "res://rust/silica-engine/target/release/libsilica_godot.dylib"
