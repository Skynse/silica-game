[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1

[libraries]
linux.debug.x86_64 = "res://rust/silica_engine/target/debug/libsilica_engine.so"
linux.release.x86_64 = "res://rust/silica_engine/target/target/release/libsilica_engine.so"
windows.debug.x86_64 = "res://rust/silica_engine/target/target/debug/silica_engine.dll"
windows.release.x86_64 = "res://rust/silica_engine/target/target/release/silica_engine.dll"
macos.debug = "res://rust/silica_engine/target/debug/libsilica_engine.dylib"
macos.release = "res://rust/silica_engine/target/release/libsilica_engine.dylib"
macos.debug.arm64 = "res://rust/silica_engine/target/debug/libsilica_engine.dylib"
macos.release.arm64 = "res://rust/silica_engine/target/release/libsilica_engine.dylib"
